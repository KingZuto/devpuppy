# Copy this file to terraform.tfvars and fill in your values

# GitHub repository URL (e.g., https://github.com/username/devpuppy)
github_repository = "https://github.com/YOUR_USERNAME/YOUR_REPO"

# GitHub personal access token with repo permissions
github_access_token = "ghp_your_github_token_here"

# Optional: Custom domain name
# domain_name = "your-domain.com"

# AWS region (default: ap-northeast-2 for Seoul)
# aws_region = "ap-northeast-2"
